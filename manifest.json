{
    "manifest_version": 3,
    "name": "Croboost",
    "description": "Lets you remix pages from all over the internet",
    "version": "1.0",
    "permissions": [
      "activeTab"
    ],
    "icons": {
      "46": "images/logo.png"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["index.js"]
      }
    ],
    "web_accessible_resources": [
      {
        "resources": ["images/logo.svg", "images/ai-icon.svg", "js-confetti.min.js"],
        "matches": ["<all_urls>"]
      }
    ]
  }